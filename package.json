    "url-loader": "4.1.1",
    "webpack": "4.32.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
